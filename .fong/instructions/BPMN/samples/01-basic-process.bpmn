<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  id="Definitions_BasicProcess"
  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:process id="Process_BasicLinear" name="Basic Linear Process" isExecutable="false">

    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Process&#10;Started">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Task 1 -->
    <bpmn:task id="Task_1" name="Receive&#10;Request">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <!-- Task 2 -->
    <bpmn:task id="Task_2" name="Process&#10;Data">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <!-- Task 3 -->
    <bpmn:task id="Task_3" name="Send&#10;Response">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>

    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_1" name="Process&#10;Completed">
      <bpmn:incoming>Flow_4</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_1"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_1" targetRef="Task_2"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_2" targetRef="Task_3"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_3" targetRef="EndEvent_1"/>

  </bpmn:process>

  <!-- Diagram Information -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_BasicLinear">

      <!-- Start Event Shape -->
      <bpmndi:BPMNShape id="Shape_StartEvent_1" bpmnElement="StartEvent_1">
        <dc:Bounds x="150" y="192" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="141" y="235" width="54" height="27"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Task 1 Shape -->
      <bpmndi:BPMNShape id="Shape_Task_1" bpmnElement="Task_1">
        <dc:Bounds x="250" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Task 2 Shape -->
      <bpmndi:BPMNShape id="Shape_Task_2" bpmnElement="Task_2">
        <dc:Bounds x="410" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Task 3 Shape -->
      <bpmndi:BPMNShape id="Shape_Task_3" bpmnElement="Task_3">
        <dc:Bounds x="570" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <!-- End Event Shape -->
      <bpmndi:BPMNShape id="Shape_EndEvent_1" bpmnElement="EndEvent_1">
        <dc:Bounds x="732" y="192" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="719" y="235" width="62" height="27"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Flow 1 -->
      <bpmndi:BPMNEdge id="Edge_Flow_1" bpmnElement="Flow_1">
        <di:waypoint x="186" y="210"/>
        <di:waypoint x="250" y="210"/>
      </bpmndi:BPMNEdge>

      <!-- Flow 2 -->
      <bpmndi:BPMNEdge id="Edge_Flow_2" bpmnElement="Flow_2">
        <di:waypoint x="350" y="210"/>
        <di:waypoint x="410" y="210"/>
      </bpmndi:BPMNEdge>

      <!-- Flow 3 -->
      <bpmndi:BPMNEdge id="Edge_Flow_3" bpmnElement="Flow_3">
        <di:waypoint x="510" y="210"/>
        <di:waypoint x="570" y="210"/>
      </bpmndi:BPMNEdge>

      <!-- Flow 4 -->
      <bpmndi:BPMNEdge id="Edge_Flow_4" bpmnElement="Flow_4">
        <di:waypoint x="670" y="210"/>
        <di:waypoint x="732" y="210"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
