<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.40.1">
  <bpmn:process id="Process_AllTasks" isExecutable="false">

    <bpmn:startEvent id="StartEvent" name="Start">
      <bpmn:outgoing>Flow1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Task Types -->
    <bpmn:task id="Task_Generic" name="Generic Task">
      <bpmn:incoming>Flow1</bpmn:incoming>
      <bpmn:outgoing>Flow2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:userTask id="UserTask" name="User Task&#10;(Review Document)">
      <bpmn:incoming>Flow2</bpmn:incoming>
      <bpmn:outgoing>Flow3</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="ServiceTask" name="Service Task&#10;(API Call)">
      <bpmn:incoming>Flow3</bpmn:incoming>
      <bpmn:outgoing>Flow4</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:scriptTask id="ScriptTask" name="Script Task&#10;(Calculate)">
      <bpmn:incoming>Flow4</bpmn:incoming>
      <bpmn:outgoing>Flow5</bpmn:outgoing>
    </bpmn:scriptTask>

    <bpmn:manualTask id="ManualTask" name="Manual Task&#10;(Sign Paper)">
      <bpmn:incoming>Flow5</bpmn:incoming>
      <bpmn:outgoing>Flow6</bpmn:outgoing>
    </bpmn:manualTask>

    <bpmn:businessRuleTask id="BusinessRuleTask" name="Business Rule&#10;(Approve Loan)">
      <bpmn:incoming>Flow6</bpmn:incoming>
      <bpmn:outgoing>Flow7</bpmn:outgoing>
    </bpmn:businessRuleTask>

    <bpmn:sendTask id="SendTask" name="Send Task&#10;(Email Report)">
      <bpmn:incoming>Flow7</bpmn:incoming>
      <bpmn:outgoing>Flow8</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:receiveTask id="ReceiveTask" name="Receive Task&#10;(Wait Response)">
      <bpmn:incoming>Flow8</bpmn:incoming>
      <bpmn:outgoing>Flow9</bpmn:outgoing>
    </bpmn:receiveTask>

    <!-- Sub-Process -->
    <bpmn:subProcess id="SubProcess" name="Sub-Process&#10;(Order Fulfillment)">
      <bpmn:incoming>Flow9</bpmn:incoming>
      <bpmn:outgoing>Flow10</bpmn:outgoing>
    </bpmn:subProcess>

    <!-- Call Activity -->
    <bpmn:callActivity id="CallActivity" name="Call Activity&#10;(Reusable Process)">
      <bpmn:incoming>Flow10</bpmn:incoming>
      <bpmn:outgoing>Flow11</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:endEvent id="EndEvent" name="End">
      <bpmn:incoming>Flow11</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow1" sourceRef="StartEvent" targetRef="Task_Generic" />
    <bpmn:sequenceFlow id="Flow2" sourceRef="Task_Generic" targetRef="UserTask" />
    <bpmn:sequenceFlow id="Flow3" sourceRef="UserTask" targetRef="ServiceTask" />
    <bpmn:sequenceFlow id="Flow4" sourceRef="ServiceTask" targetRef="ScriptTask" />
    <bpmn:sequenceFlow id="Flow5" sourceRef="ScriptTask" targetRef="ManualTask" />
    <bpmn:sequenceFlow id="Flow6" sourceRef="ManualTask" targetRef="BusinessRuleTask" />
    <bpmn:sequenceFlow id="Flow7" sourceRef="BusinessRuleTask" targetRef="SendTask" />
    <bpmn:sequenceFlow id="Flow8" sourceRef="SendTask" targetRef="ReceiveTask" />
    <bpmn:sequenceFlow id="Flow9" sourceRef="ReceiveTask" targetRef="SubProcess" />
    <bpmn:sequenceFlow id="Flow10" sourceRef="SubProcess" targetRef="CallActivity" />
    <bpmn:sequenceFlow id="Flow11" sourceRef="CallActivity" targetRef="EndEvent" />

  </bpmn:process>

